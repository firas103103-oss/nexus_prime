{
  "name": "arc-namer-vscode",
  "version": "1.0.0",
  "description": "Visual Studio Code extension for Arc Namer",
  "main": "src/extension.ts",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "keywords": [
    "vscode",
    "extension"
  ],
  "author": "firas103103-oss",
  "license": "MIT",
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "typescript": "^4.4.3",
    "eslint": "^7.32.0",
    "jest": "^27.0.6"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:arc-namer-vscode.start"
  ],
  "contributes": {
    "commands": [
      {
        "command": "arc-namer-vscode.start",
        "title": "Start Arc Namer"
      }
    ]
  }
}